# WalletConnect Project ID - Get yours from https://cloud.walletconnect.com/
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# Contract Configuration
NEXT_PUBLIC_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_NETWORK=arbitrumSepolia

# Private key for contract deployment (DO NOT COMMIT THIS)
PRIVATE_KEY=your_private_key_here

# Enable testnets (optional)
NEXT_PUBLIC_ENABLE_TESTNETS=true

# API Keys for contract verification (optional)
ETHERSCAN_API_KEY=your_etherscan_api_key
BSCSCAN_API_KEY=your_bscscan_api_key
SNOWSCAN_API_KEY=your_snowscan_api_key
ARBISCAN_API_KEY=your_arbiscan_api_key
OPTIMISM_API_KEY=your_optimism_api_key

# API URLs for contract verification
ETHERSCAN_API_URL=https://api.etherscan.io/api
BSCSCAN_API_URL=https://api.bscscan.com/api
SNOWSCAN_API_URL=https://api.snowscan.xyz/api
ARBISCAN_API_URL=https://api.arbiscan.io/api
OPTIMISM_API_URL=https://api-optimistic.etherscan.io/api